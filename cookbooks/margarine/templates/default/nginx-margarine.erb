server {
        listen          80;
        server_name     margarine.raxsavvy.com;

        access_log /var/log/nginx/margarine_access.log;
        error_log /var/log/nginx/margarine_error.log;

        location / {
            include         uwsgi_params;
            uwsgi_pass      127.0.0.1:3031;
        }
}
